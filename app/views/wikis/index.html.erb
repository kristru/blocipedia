<h1>Blocipedia Wikis</h1>

 <div class="row">
   <div class="col-md-8">
     <% @wikis.each do |wiki| %>
       <div class="media list">
         <div class="media-body">
           <h4 class="media-heading">
             <%= link_to wiki.title, wiki %>
           </h4>
           <small>
             <%= wiki.body %>
           </small>
         </div>
       </div>
     <% end %>
   </div>
 </div>
